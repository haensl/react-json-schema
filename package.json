{
  "name": "@haensl/react-json-schema",
  "version": "1.0.0",
  "description": "Simple JSON+LD schema React component.",
  "type": "module",
  "module": "index.js",
  "scripts": {
    "lint": "eslint '**/*.js'",
    "lint:junit": "eslint --format junit -o test-results/eslint/results.xml '**/*.js'",
    "prepare": "if [ \"${NODE_ENV}\" != 'production' ] && [ -z \"${CI}\" ]; then husky install; fi",
    "test": "jest --runInBand --no-cache --colors --passWithNoTests",
    "test:junit": "jest --runInBand --no-cache --ci --reporters='default' --reporters='jest-junit' --passWithNoTests",
    "tdd": "jest --runInBand --colors --watch"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "extends": [
      "@haensl",
      "react-app"
    ],
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "jest": {
    "globals": {
      "NODE_ENV": "test"
    },
    "verbose": true,
    "clearMocks": true,
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "jest-junit": {
    "outputDirectory": "test-results/jest",
    "outputName": "results.xml",
    "suitName": "@haensl/next-google-analytics unit tests"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/haensl/react-json-schema.git"
  },
  "keywords": [
    "schema",
    "json+ld",
    "jsonld",
    "microdata",
    "react",
    "component"
  ],
  "author": {
    "name": "HP Dietz",
    "url": "https://hpdietz.com",
    "twitter": "@h_p_d",
    "email": "h.p.dietz@gmail.com"
  },
  "funding": "https://github.com/sponsors/haensl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haensl/react-json-schema/issues"
  },
  "homepage": "https://github.com/haensl/react-json-schema#readme",
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@haensl/eslint-config": "^1.4.6",
    "@testing-library/react": "^14.0.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.43.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-junit": "^16.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@haensl/mimetypes": "^1.0.1"
  }
}
